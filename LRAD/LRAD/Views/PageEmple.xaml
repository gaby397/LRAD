<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="LRAD.Views.PageEmple">
    <ContentPage.Content>
        <StackLayout>
            
            <Entry x:Name="txtcodigo" Placeholder="Ingrese codigo" Keyboard="Text" />
            <Entry x:Name="txtnombres" Placeholder="Ingrese Nombres" Keyboard="Text" />
            <Entry x:Name="txtapellidos" Placeholder="Ingrese apellidos" Keyboard="Text" />
            <Entry x:Name="txttelefono" Placeholder="Ingrese telefono" Keyboard="Telephone" />
            <Entry x:Name="txtcorreo" Placeholder="Ingrese e-mail" Keyboard="Email" />
            <Button x:Name="btnagregar" Text="Salvar" Clicked="btnagregar_Clicked"/>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>